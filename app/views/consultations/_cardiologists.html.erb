 <div data-search-cardio-target="list">
 
  <% @cardiologists&.each do |cardiologist| %>
    <div class="bg-white my-2 py-2 px-1 rounded" data-action="click->search-cardio#selectcardio" value="<%= cardiologist["Prénom d'exercice"] %>_<%= cardiologist["Nom d'exercice"] %>">
      <div><%= cardiologist["Libellé civilité d'exercice"] %> <%= cardiologist["Nom d'exercice"] %> <%= cardiologist["Prénom d'exercice"] %></div>
      <span><%= cardiologist["Numéro Voie"] %> <%= cardiologist["Libellé Voie"] %> <%= cardiologist["Code Postal"] %> <%= cardiologist["Libellé commune"] %></span>
    </div>
  <% end %>
 </div>


  